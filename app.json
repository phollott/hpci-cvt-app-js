{
  "expo": {
    "owner": "hpci-cvt",
    "name": "HPCI CVT",
    "description": "HPCI COVID-19 Vaccine and Treatments App",
    "slug": "hpci-cvt-app-js",
    "privacy": "public",
    "version": "0.0.45",
    "platforms": ["android", "ios", "web"],
    "githubUrl": "https://github.com/phollott/hpci-cvt-app-js",
    "orientation": "default",
    "primaryColor": "#2a2a2a",
    "icon": "src/assets/icon.png",
    "splash": {
      "backgroundColor": "#ffffff",
      "image": "src/assets/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "src/assets/**/*",
      "node_modules/@expo/vector-icons/**/*"
    ],
    "android": {
      "package": "com.hpcicvt.hpcicvtappjs",
      "versionCode": 45,
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true
    },
    "ios": {
      "bundleIdentifier": "com.hpcicvt.hpcicvtappjs",
      "buildNumber": "0.0.45",
      "supportsTablet": true
    }
  }
}
